<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sheep.androidanntationssample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.sheep.androidanntationssample.MainActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.sheep.androidanntationssample.BeforeActivity"
            android:label="@string/title_activity_before" >
        </activity>
        <activity
            android:name="org.sheep.androidanntationssample.AfterActivity_"
            android:label="@string/title_activity_after" >
        </activity>
    </application>

</manifest>
